<h1>{{ title }}</h1>

<table>
  <tr>
    <td>Student_Id</td>
    <td>Class_Id</td>
  </tr>
  {{#each enrollment}}
    <form method="post">
      <tr>
        <td>{{Student_Id}}</td>
        <td>{{Class_Id}}</td>

        <input type="hidden" name="studentId" value="{{Student_Id}}">
        <input type="hidden" name="classId" value="{{Class_Id}}">
        
        <td style="border: none; margin-left: 10px;">
          <button 
            style="margin-left: 10px;" 
            type="submit" 
          >
          delete
          </button>
        </td>
      </tr>    
    </form>
  {{/each}}
</table>